    "start:node": "node ./bin/run",
    "build": "tsup-node",
    "build:watch": "tsup-node --watch"
  },
  "dependencies": {
    "consola": "^3.4.2",
    "destr": "^2.0.5",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.13.1",
    "eslint": "^9.26.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  }
}
